<title>View</title>


<body>

<div class="container mt-5  ">
<div class="row d-flex justify-content-center align-items-center">
    <div class="col-md-8  border border-white p-4 rounded bg-white" >
      <form >
        <div class="form-row">
          <div class="form-group col-md-5">
            <label for="inputEmail4">Last Name</label>
            <input type="text" class="form-control" id="lastName" name="lastName" autocomplete="LastName" value="<%= data.lastName %>"   readonly>
          </div>

          <div class="form-group col-md-5">
            <label for="inputPassword4">First Name</label>
            <input type="text" class="form-control" id="firstName" name="firstName" autocomplete="firstName" value="<%= data.firstName %>"   readonly>
          </div>

          <div class="form-group col-md-5">
            <label for="inputPassword4">Middle Name</label>
            <input type="text" class="form-control" id="middleName" name="middleName" autocomplete="middleName" value="<%= data.middleName %>"   readonly>
          </div>

          <div class="form-group col-md-5">
            <label for="inputPassword4">QLFR</label>
            <input type="text" class="form-control" id="QLFR" name="QLFR" autocomplete="QLFR" value="<%= data.QLFR %>"   readonly>
          </div>

          <div class="form-group col-md-2">
            <label for="inputZip">Rank</label>
            <input type="text" class="form-control" id="rank" name="rank" value="<%= data.rank %>" readonly>
          </div>

          <div class="form-group col-md-5">
            <label for="inputEmail4">Station</label>
            <input type="text" class="form-control" id="station" name="station" autocomplete="station" value="<%= data.station %>"   readonly>
          </div>

          <div class="form-group col-md-5">
            <label for="inputEmail4">Gun Type</label>
            <input type="text" class="form-control" id="Gtype" name="Gtype" autocomplete="Gtype" value="<%= data.Gtype %>"   readonly>
          </div>

          <div class="form-group col-md-5">
            <label for="inputEmail4">Gun Name</label>
            <input type="text" class="form-control"  id="Gname" name="Gname" autocomplete="Gname" value="<%= data.Gname %>"   readonly>
          </div>

          <div class="form-group col-md-5">
            <label for="inputEmail4">Caliber</label>
            <input type="text" class="form-control" id="caliber" name="caliber" autocomplete="caliber" value="<%= data.caliber %>" readonly>
          </div>

          <div class="form-group col-md-5">
            <label for="inputEmail4">Serial Number</label>
            <input type="text" class="form-control" id="serialN" name="serialN" autocomplete="serialN" value="<%= data.serialN %>"  readonly>
          </div>

          <div class="form-group col-md-5">
            <label for="inputEmail4">Cost</label>
            <input type="text" class="form-control" id="cost" name="cost" autocomplete="cost" value="<%= data.cost %>"  readonly>
          </div>

          <div class="form-group col-md-5">
            <label for="inputEmail4">Acquisition date</label>
            <input type="date" class="form-control" id="acquisition" name="acquisition" value="<%= data.acquisition %>" readonly>
          </div>

          <div class="form-group col-md-5">
            <label for="inputEmail4">Turn Over</label>
            <input type="date" class="form-control" id="turnOver" name="turnOver" autocomplete="turnOver" value="<%= data.turnOver %>"  readonly>
          </div>

          <div class="form-group col-md-5">
            <label for="inputEmail4">Returned</label>
            <input type="date" class="form-control" id="returned" name="returned" autocomplete="returned" value="<%= data.returned %>"  readonly>
          </div>
        </div>

      </form>
    </div>
</div>
</div> 

</body>
</html>
        
